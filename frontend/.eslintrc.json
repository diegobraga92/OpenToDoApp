{
  "root": true,
  "extends": [
    "expo",
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "es2021": true,
    "node": true,
    "browser": true
  },
  "rules": {
    "no-unused-vars": "off",
    "react/react-in-jsx-scope": "off",
    "react/jsx-uses-react": "off"
  }
}
